    "lint-staged": "^8.1.6",
    "node-sass": "^4.11.0",
    "portfinder": "^1.0.20",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "react-dev-utils": "^8.0.0",
    "sass-loader": "^7.1.0",
    "scss-loader": "0.0.1",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
