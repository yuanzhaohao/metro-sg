    "babel-preset-stage-0": "^6.24.1",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "express": "^4.16.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
